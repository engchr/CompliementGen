{
    "builds": [
      { "src": "api/AdvancedComps.js", "use": "@vercel/node" },
      { "src": "public/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/generateCompliment", "dest": "/api/AdvancedComps.js" },
      { "src": "/index", "dest": "/public/index.html" },
      { "src": "/AdvancedComp", "dest": "/public/AdvancedComp.html" },
      { "src": "/tiktoks", "dest": "/public/tiktoks.html" },
      { "src": "/(.*)", "dest": "/public/index.html" }
    ]
  }
  