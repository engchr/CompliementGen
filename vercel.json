{
    "builds": [
      { "src": "api/generateCompliment.js", "use": "@vercel/node" },
      { "src": "public/index.html", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/generateCompliment.js" },
      { "src": "/(.*)", "dest": "/public/index.html" }
    ]
  }
  