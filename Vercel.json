{
    "version": 2,
    "builds": [
      {
        "src": "src/components/AdvancedComps.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/scripts/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/public/(.*)", "dest": "/public/$1" },
      { "src": "/(.*)", "dest": "/public/index.html" }
    ]
  }
  