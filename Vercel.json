{
    "version": 2,
    "builds": [
      {
        "src": "pages/*.html",
        "use": "@vercel/static"
      },
      {
        "src": "Source/Styles/*.css",
        "use": "@vercel/static"
      },
      {
        "src": "Source/Scripts/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/styles/(.*)", "dest": "/Source/Styles/$1" },
      { "src": "/scripts/(.*)", "dest": "/Source/Scripts/$1" },
      { "src": "/(.*)", "dest": "/pages/$1" }
    ]
  }
  